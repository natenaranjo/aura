 <div class="w-screen h-screen flex flex-col justify-center items-center gap-16">
    <h1 class="text-6xl">{{user.name}}</h1>
    <p class="text-2xl">User Id = {{user.id}}</p>
    <button class="bg-blue-500 hover:bg-blue-700 text-white font-bold py-2 px-4 rounded" id="create-entry">Create Entry</button>
    <button class="bg-blue-500 hover:bg-blue-700 text-white font-bold py-2 px-4 rounded" id="all-entries">View All Entries</button>
    <button class="bg-blue-500 hover:bg-blue-700 text-white font-bold py-2 px-4 rounded" id="past-week">Past Week</button>
    <button class="bg-blue-500 hover:bg-blue-700 text-white font-bold py-2 px-4 rounded" id="past-month">Past Month</button>
    <button class="bg-red-500 hover:bg-blue-700 text-white font-bold py-2 px-4 rounded" id="logout">Log Out</button>
 </div>


